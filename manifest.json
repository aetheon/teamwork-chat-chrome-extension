{
  "manifest_version": 2,

  "name": "Teamwork Chat",
  "description": "Displays Teamwork Chat Notifications",
  "version": "1.0",

  "background": {
    "scripts": ["eventPage.js"],
    "persistent": false
  },

  "browser_action": {
    "default_icon": "res/16x16icon.png",
    "default_title": "Teamwork Chat"


  },
  "content_scripts": [
    {
      "axios": ["js/axios.min.js"]
    }
  ],
  "permissions": [
    "activeTab",
    "storage"
  ]
}